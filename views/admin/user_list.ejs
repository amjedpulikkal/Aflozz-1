<div class="product-status mg-b-30">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <div class="product-status-wrap">
                    <h4>user List</h4>
                    <!-- <div class="add-product">
                        <a href="/admin/add/category">Add category</a>
                    </div> -->
                    <table>
                        <tr>
                            <th>Name</th>
                            <th>Email</th>
                            <th>Status</th>
                            <th>Setting</th>
                        </tr>
                        <% user.forEach(function(item) { %>
                            <tr>
                                <td>
                                    <%= item.Name%>
                                </td>
                                <td>
                                    <%= item.Email%>
                                </td>
                                <td>
                                    <%if(item.presence === "online"){%>
                                   <p class="text-green-600" id="Status<%=item._id%>">Online</p>
                                   <%}else{%>
                                    <p id="Status<%=item._id%>"><%=item.presence%></p>
                                    <%}%>
                                </td>
                                <td>
                                    <% if(item.status===false){ %>
                                        <a href="/admin/block?_id=<%=item._id%>&status=true" class="ds-setting">Disabled</a>
                                    <%}else{%>
                                            <a href="/admin/block?_id=<%=item._id%>&status=false" class="pd-setting">Active</a>
                                        <%}%>
                                </td>
                            </tr>
                            <% }); %>
                    </table>
                    <!-- <div class="custom-pagination">
                        <ul class="pagination">
                            <li class="page-item"><a class="page-link" href="#">Previous</a></li>
                            <li class="page-item"><a class="page-link" href="#">1</a></li>
                            <li class="page-item"><a class="page-link" href="#">2</a></li>
                            <li class="page-item"><a class="page-link" href="#">3</a></li>
                            <li class="page-item"><a class="page-link" href="#">Next</a></li>
                        </ul>
                    </div> -->
                </div>
            </div>
        </div>
    </div>
</div>
